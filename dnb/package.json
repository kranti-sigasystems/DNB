{
  "name": "dnb",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:strict": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "merge:prisma": "tsx src/lib/utils/merge-prisma.ts",
    "prisma:generate": "npm run merge:prisma && npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:studio": "npm run merge:prisma && npx prisma studio",
    "prisma:reset": "npx prisma migrate reset",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed-plans": "tsx scripts/seed-plans.ts",
    "db:setup-buyers": "tsx scripts/setup-buyers-table.ts",
    "db:setup-products-locations": "tsx scripts/setup-products-locations.ts",
    "db:setup-all": "npm run db:seed-plans && npm run db:setup-products-locations && npm run db:setup-buyers",
    "type-check": "tsc --noEmit"
  },
